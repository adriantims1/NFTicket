{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "NFTicket",
    "slug": "NFTicket",
    "description":"The app for your ticket needs",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation":"portrait",
    "icon":"./assets/images/icon.png",
